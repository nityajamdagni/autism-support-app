#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Nitya/autism-app/node_modules/.pnpm/vite-node@3.2.4_jiti@1.21.7_yaml@2.8.1/node_modules/vite-node/node_modules:/mnt/c/Users/Nitya/autism-app/node_modules/.pnpm/vite-node@3.2.4_jiti@1.21.7_yaml@2.8.1/node_modules:/mnt/c/Users/Nitya/autism-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Nitya/autism-app/node_modules/.pnpm/vite-node@3.2.4_jiti@1.21.7_yaml@2.8.1/node_modules/vite-node/node_modules:/mnt/c/Users/Nitya/autism-app/node_modules/.pnpm/vite-node@3.2.4_jiti@1.21.7_yaml@2.8.1/node_modules:/mnt/c/Users/Nitya/autism-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite-node@3.2.4_jiti@1.21.7_yaml@2.8.1/node_modules/vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../../../vite-node@3.2.4_jiti@1.21.7_yaml@2.8.1/node_modules/vite-node/vite-node.mjs" "$@"
fi
